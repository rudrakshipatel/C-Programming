#include <stdio.h>

int main() {
    int a[5], i, j, temp;
    printf("Enter 5 values:\n");
    for(i = 0; i < 5; i++)
        scanf("%d", &a[i]);

    // Bubble Sort (simple and clear)
    for(i = 0; i < 4; i++) {
        for(j = 0; j < 4 - i; j++) {
            if(a[j] > a[j + 1]) { // ascending
                temp = a[j];
                a[j] = a[j + 1];
                a[j + 1] = temp;
            }
        }
    }

    printf("Array in ascending order:\n");
    for(i = 0; i < 5; i++)
        printf("%d ", a[i]);
    return 0;
}
