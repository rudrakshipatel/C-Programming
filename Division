#include <stdio.h>

int main() {
    float a, b, result;

    printf("Enter two numbers: ");
    scanf("%f %f", &a, &b);

    if (b != 0)
        result = a / b;
    else {
        printf("Division by zero is not allowed.\n");
        return 0;
    }

    printf("Quotient = %.2f\n", result);
    return 0;
}
