#include <stdio.h>
#include <ctype.h>

int main() {
    char str[200];
    int i;
    int a = 0, e = 0, i_count = 0, o = 0, u = 0;

    printf("Enter a string: ");
    gets(str);

    for(i = 0; str[i] != '\0'; i++) {
        char ch = tolower(str[i]);
        if(ch == 'a') a++;
        else if(ch == 'e') e++;
        else if(ch == 'i') i_count++;
        else if(ch == 'o') o++;
        else if(ch == 'u') u++;
    }

    printf("Frequency of vowels:\n");
    printf("A = %d\nE = %d\nI = %d\nO = %d\nU = %d\n", a, e, i_count, o, u);
    return 0;
}
